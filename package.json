{
  "name": "constellation",
  "version": "0.1.0",
  "description": "Desktop application for visualizing and orchestrating AI agent swarms",
  "main": "dist-electron/main/index.js",
  "author": "Shaal",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "package": "electron-builder --config electron-builder.yml",
    "package:mac": "electron-builder --mac --config electron-builder.yml",
    "package:win": "electron-builder --win --config electron-builder.yml",
    "package:linux": "electron-builder --linux --config electron-builder.yml",
    "test": "vitest run",
    "test:unit": "vitest run --config vitest.config.unit.ts",
    "test:e2e": "playwright test",
    "test:watch": "vitest",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "typecheck": "tsc --noEmit && tsc --noEmit -p tsconfig.node.json && tsc --noEmit -p tsconfig.web.json"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.26.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@types/glob": "^8.1.0",
    "@xyflow/react": "^12.0.0",
    "better-sqlite3": "^11.0.0",
    "clsx": "^2.1.0",
    "electron-updater": "^6.1.7",
    "framer-motion": "^11.0.0",
    "glob": "^13.0.0",
    "immer": "^10.0.0",
    "lucide-react": "^0.400.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tailwind-merge": "^2.3.0",
    "uuid": "^10.0.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.6.0",
    "@playwright/test": "^1.44.0",
    "@types/better-sqlite3": "^7.6.10",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "electron": "^30.0.0",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.4.0",
    "vite": "^5.0.0",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
